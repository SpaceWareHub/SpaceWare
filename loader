    MainWindow2:Toggle('Double Coins',function(state)
        if state == true then
            require(game:GetService("ReplicatedStorage").Mutators["double coins"]).mutate()
        else
            require(game:GetService("ReplicatedStorage").Mutators["double coins"]).revert()
        end
    end)
    
    MainWindow2:Toggle('Double Jump',function(state)
        if state == true then
            require(game:GetService("ReplicatedStorage").Mutators["double jump"]).mutate()
        else
            require(game:GetService("ReplicatedStorage").Mutators["double jump"]).revert()
        end
    end)
    
